---
import ExampleGrid from "@/components/example-grid.astro";
import PageHeader from "@/components/page-header.astro";
import { navMenuConfig } from "@/config/nav-menu";
import MainLayout from "@/layouts/main-layout.astro";


const OpenUSD = navMenuConfig.OpenUSD[0].items;
const OpenUSD2 = navMenuConfig.OpenUSD[0].items;
---

<MainLayout title="">
  <div class="bg-background-200">
    <section class="container py-12">
      <PageHeader
        heading="Discovery"
        text="Differents pages and examples apps built with Astro v4.5, shadcn/ui & react js. Fully OpenUSD Source."
      />

      <ExampleGrid title="OpenUSD" items={OpenUSD} />
      <ExampleGrid title="OpenUSD2" items={OpenUSD2} />
      
    </section>
  </div>
</MainLayout>
